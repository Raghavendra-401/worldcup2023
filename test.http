DELETE http://localhost:4004/odata/v2/world-cup/TeamsData('West Indies')

###UPDATE - TEAMSDATA
PUT http://localhost:4004/odata/v2/world-cup/TeamsData('India')
Content-Type: application/json

{
    "MatchesPlayed": 4,
    "MatchesWon": 4,
    "MatchesLost": 0,
    "MatchesTied": 0,
    "Points": 8
}

